<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.bookshop.user.entity.Withdraw" table="s_withdraw" schema="second-hand bookshop">
        <id name="id">
            <column name="id"></column>
            <generator class="native"></generator>
        </id>
        <property name="withdrawId"></property>
        <property name="sellerEmail"></property>
        <property name="alipayNum"></property>
        <property name="creatTime"></property>
        <property name="withdrawMoney"></property>
        <property name="withdrawStatus"></property>
    </class>
</hibernate-mapping>